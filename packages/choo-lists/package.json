{
  "name": "choo-lists",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production bankai build index.js",
    "create": "choo-scaffold",
    "inspect": "bankai inspect index.js",
    "start": "bankai start index.js",
    "test": "standard && npm run test-deps",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons"
  },
  "prettier": {
    "printWidth": 70,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "jsxBracketSameLine": false
  },
  "jest": {
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "choo": "6.11.0",
    "choo-service-worker": "2.4.0",
    "choo-store": "0.6.0",
    "codemirror": "5.38.0",
    "dom-autofocus": "1.0.1",
    "faker": "4.1.0",
    "firebase": "5.0.4",
    "js-yaml": "3.11.0",
    "kefir": "3.8.1",
    "messenger-hubspot": "1.5.0-b2",
    "mobx": "3.4.1",
    "mobx-state-tree": "1.3.1",
    "nanoid": "1.0.2",
    "nanologger": "1.3.1",
    "p-min-delay": "2.0.0",
    "p-reflect": "1.0.0",
    "plur": "3.0.1",
    "pouchdb-browser": "6.4.3",
    "ramda": "0.25.0",
    "ramda-adjunct": "2.8.0",
    "sheetify": "7.3.2",
    "tachyons": "4.9.1"
  },
  "devDependencies": {
    "@sinonjs/referee": "2.1.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-power-assert": "2.0.0",
    "babel-preset-stage-0": "6.24.1",
    "bankai": "9.12.1",
    "choo-devtools": "2.5.1",
    "choo-scaffold": "1.1.2",
    "dependency-check": "3.1.0",
    "jest": "23.0.0",
    "jest-watch-typeahead": "0.1.0",
    "minify-stream": "1.1.0",
    "p-each-series": "1.0.0",
    "power-assert": "1.5.0",
    "sinon": "4.5.0",
    "standard": "11.0.1"
  }
}
