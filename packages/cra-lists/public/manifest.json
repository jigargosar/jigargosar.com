{
  "short_name": "React App",
  "name": "React Extension",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "React Ext"
  },
  "permissions": [
    "activeTab"
  ],
  "version": "1.0",
  "content_security_policy": "script-src 'self' https://apis.google.com ; object-src 'self'; connect-src 'self' https://www.googleapis.com"
}
