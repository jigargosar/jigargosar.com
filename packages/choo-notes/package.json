{
  "name": "choo-notes",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prettier-watch": "onchange '**/*.js' 'package.json' -- prettier --write {{changed}}",
    "watch": "npm run prettier-watch",
    "build": "bankai build index.js",
    "create": "choo-scaffold",
    "inspect": "bankai inspect index.js",
    "start": "bankai start index.js",
    "test": "standard && npm run test-deps",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": false,
    "jsxBracketSameLine": false
  },
  "dependencies": {
    "chance": "1.0.16",
    "choo": "6.11.0-preview1",
    "choo-service-worker": "2.4.0",
    "faker": "4.1.0",
    "ramda": "0.25.0",
    "sheetify": "7.3.2",
    "tachyons": "4.9.1",
    "xstate": "3.2.1"
  },
  "devDependencies": {
    "bankai": "9.12.1",
    "choo-devtools": "2.5.0",
    "choo-scaffold": "1.1.2",
    "dependency-check": "3.1.0",
    "prettier": "1.12.1",
    "standard": "11.0.1"
  }
}
